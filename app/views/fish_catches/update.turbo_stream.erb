<turbo-stream action="update" target="stats">
  <template>
    <%= render partial: "tackle_box_items/stats",
        formats: [:html],
        locals: { fish_catches: @fish_catches } %>
  </template>
</turbo-stream>

<turbo-stream action="replace" target="<%= dom_id(@fish_catch) %>">
  <template>
    <%= render partial: "fish_catches/fish_catch",
        formats: [:html],
        locals: { fish_catch: @fish_catch } %>
  </template>
</turbo-stream>

<turbo-stream action="update" target="flash">
  <template>
    <%= render partial: "layouts/flash", formats: [:html] %>
  </template>
</turbo-stream>
